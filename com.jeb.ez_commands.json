{
    "app-id" : "com.jeb.ez_commands",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "master",
    "sdk" : "org.gnome.Sdk",
    "command" : "ez_commands",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--filesystem=xdg-documents"
    ],
    "permissions": {
        "filesystems": [
            {
                "path": "xdg-documents",
                "readwrite": true
            }
        ],
        "system-theme": true
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "ez_commands",
            "builddir" : true,
            "buildsystem" : "meson",
            "config-opts": [
                "-Dgtk=4.0"
            ],
            "sources" : [
                {
                    "type" : "git",
                    "url" : "file:///home/jeb192004/Projects"
                }
            ]
        }
    ]
}
